{
  "name": "@developmentcubed/nicehash-api",
  "version": "1.1.0",
  "description": "Wrapper for nicehash api",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "tag-matches",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevelopmentCubed/nicehash-api.git"
  },
  "keywords": [
    "nicehash",
    "api"
  ],
  "author": "Hazed SPaCEx",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/DevelopmentCubed/nicehash-api/issues"
  },
  "homepage": "https://github.com/DevelopmentCubed/nicehash-api#readme",
  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "check-tag-matches": "^1.0.0",
    "prettier": "^2.6.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3"
  }
}
